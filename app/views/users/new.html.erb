<%= form_for @user, url: {action: :create} do |f| %>
  <%= f.label :uid, 'UID' %>
  <%= f.text_field :uid %>

  <%= f.label :pass, 'PASS' %>
  <%= f.password_field :pass %>

  <%= f.label :age, 'AGE' %>
  <%= f.text_field :age %>

  <%= f.submit "登録" %>
<% end %>


